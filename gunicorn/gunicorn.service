[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/commodities_price
ExecStart=/home/ubuntu/commodities_price/venv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/run/gunicorn.sock \
          commodities_price.wsgi:application
[Install]
WantedBy=multi-user.target
